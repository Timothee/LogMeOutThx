{
  "name": "LogMeOutThx",
  "version": "0.5.4",
  "manifest_version": 2,
  "description": "Adds a keyboard shortcut to log you out of the current page. Pressing Alt+\\ will log you out (or try to at least). See options to change the shortcut.",
  "icons": {
    "16": "images/logmeoutthx_16.png",
    "48": "images/logmeoutthx_48.png",
    "128": "images/logmeoutthx_128.png"
  },
  "homepage_url": "http://logmeoutthx.com",
  "background": {
    "scripts": ["background.js"]
  },
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content_script.js"],
      "all_frames": true
    }
  ],
  "page_action": {
    "default_icon": "images/logmeoutthx_16.png",
    "default_title": "LogMeOutThx"
  },
  "permissions": [
    "tabs", "https://*/*", "http://*/*"
  ],
  "update_url": "http://logmeoutthx.com/chrome/updates.xml"
}