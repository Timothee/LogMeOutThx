{
  "name": "LogMeOutThx",
  "version": "0.5",
  "manifest_version": 2,
  "description": "Adds a keyboard shortcut to log you out. Pressing Alt+\\ (the default) will log you out (or try to). See options to change the shortcut.",
  "homepage_url": "http://logmeoutthx.com",
  "background": {
    "scripts": ["background.js"]
  },
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content_script.js"],
      "all_frames": true
    }
  ],
  "permissions": [
    "tabs", "https://*/*", "http://*/*"
  ],
  "update_url": "http://logmeoutthx.com/chrome/updates.xml"
}